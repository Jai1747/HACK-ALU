CHIP mux8bit{                             //8-bit 2 to 1 multiplexer
    IN x[8],y[8],c;
    OUT z[8];

    PARTS:
    mux1bit(x=x[0],y=y[0],c=c,z=z[0]);
    mux1bit(x=x[1],y=y[1],c=c,z=z[1]);
    mux1bit(x=x[2],y=y[2],c=c,z=z[2]);
    mux1bit(x=x[3],y=y[3],c=c,z=z[3]);
    mux1bit(x=x[4],y=y[4],c=c,z=z[4]);
    mux1bit(x=x[5],y=y[5],c=c,z=z[5]);
    mux1bit(x=x[6],y=y[6],c=c,z=z[6]);
    mux1bit(x=x[7],y=y[7],c=c,z=z[7]);
}